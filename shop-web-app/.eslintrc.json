{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": [0, { "allow": "single-child" }]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
